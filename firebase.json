{
  "functions": {
    "source": ".output/server",
    "codebase": "nuxt"
  },
  
  "hosting": {
    "site": "ichtussite",
    "cleanUrls": true,
    "public": ".output/public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "function": "server"
      }
    ]
  }
}
